<!DOCTYPE html>
<html lang="en">
  <%- include('./partials/head.ejs') %>
  <body>
    <div class="container">
      <div class="content">
        <% if(users.length > 0 ) { %> <% users.forEach(users => { %>
        <h3 class="title"><%= users.firstName %> <%= users.lastName %></h3>
        <p class="description"><%= users.email %></p>
        <% }) %>
        <% } else { %>
        <h3 class="title">No user found</h3>
        <% } %>
      </div>
    </div>
  </body>
</html>
